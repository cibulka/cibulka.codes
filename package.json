{
  "name": "cibulka.codes",
  "version": "0.1.0",
  "private": true,
  "license": "MIT",
  "scripts": {
    "dev": "concurrently --raw \"next dev\" \"npm run watch:i18n:extract\" \"npm run watch:i18n:compile\"",
    "prebuild": "npm run i18n:extract && npm run i18n:compile",
    "build": "next build",
    "i18n:extract": "tsx lib/scripts/i18n/extract.ts",
    "i18n:compile": "tsx lib/scripts/i18n/compile.ts",
    "watch:i18n:extract": "nodemon -q --watch 'messages.ts' --exec 'npm run i18n:extract'",
    "watch:i18n:compile": "nodemon -q --watch 'messages/*.json' --exec 'npm run i18n:compile'",
    "cv": "tsx lib/scripts/cv/cv.ts",
    "start": "next start",
    "lint": "next lint"
  },
  "dependencies": {
    "@headlessui/react": "^2.1.2",
    "@vercel/analytics": "^1.3.1",
    "@vercel/og": "^0.6.2",
    "csshake": "^1.7.0",
    "dayjs": "^1.11.12",
    "next": "14.2.5",
    "next-i18n-router": "^5.5.1",
    "next-plausible": "^3.12.0",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-intl": "^6.6.8",
    "react-range": "^1.9.2",
    "react-tetris-ts": "1.0.1",
    "sharp": "^0.33.4"
  },
  "devDependencies": {
    "@formatjs/cli": "^6.2.12",
    "@types/node": "20.14.11",
    "@types/react": "18.3.3",
    "@types/react-dom": "18.3.0",
    "autoprefixer": "10.4.19",
    "concurrently": "^8.2.2",
    "contentlayer2": "^0.5.0",
    "eslint": "8.57.0",
    "eslint-config-next": "14.2.5",
    "eslint-plugin-formatjs": "^4.13.3",
    "execa": "^9.3.0",
    "next-contentlayer2": "^0.5.0",
    "nodemon": "^3.1.4",
    "ora": "^8.0.1",
    "postcss": "^8.4.39",
    "puppeteer": "^22.13.1",
    "tailwindcss": "3.4.6",
    "tsx": "^4.16.2",
    "typescript": "^5.5.3"
  }
}
